<?xml version="1.0" encoding="UTF-8"?>
<binding>
    <mapping name="HumanRequest"
		class="message.HumanRequest">
        <namespace prefix="ms" uri="http://localhost:8070/jWS/" default="all"/>
        <value name="id" field="id" />
    </mapping>

    <mapping name="HumanResponse"
		class="message.HumanResponse">
        <namespace prefix="ms" uri="http://localhost:8070/jWS/" default="all"/>
        <structure name="human" field="human" class="types.Human">
            <value name="name" field="name" />
            <value name="surname" field="surname" />
            <value name="id" field="id" />
        </structure>
    </mapping>
</binding>