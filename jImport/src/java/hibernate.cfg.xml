<hibernate-configuration>
    <session-factory>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/value4it</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.connection.password">Pass4root</property>
        <property name="hibernate.current_session_context_class">thread</property>
        <property name="hibernate.connection.characterEncoding">UTF-8</property>
        <property name="connection.provider_class">org.hibernate.connection.C3P0ConnectionProvider</property>
        <property name="connection.pool_size">20</property>
        <property name="c3p0.min_size">2</property>
        <property name="c3p0.max_size">20</property>
        <property name="c3p0.timeout">320</property>
        <property name="c3p0.max_statements">3</property>
        <property name="c3p0.idle_test_period">300</property>
