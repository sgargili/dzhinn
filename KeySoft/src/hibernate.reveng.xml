<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="softdb"/>
  <table-filter match-name="product_type"/>
  <table-filter match-name="attribute"/>
  <table-filter match-name="attribute_to_value"/>
  <table-filter match-name="product_type_to_attribute"/>
  <table-filter match-name="value"/>
  <table-filter match-name="product_type_to_value"/>
</hibernate-reverse-engineering>
