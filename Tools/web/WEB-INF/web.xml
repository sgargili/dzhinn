<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>FeXport</servlet-name>
        <servlet-class>fe.FeXport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>fe.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeXportNew</servlet-name>
        <servlet-class>fe.FeXportNew</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllParams</servlet-name>
        <servlet-class>fe.GetAllParams</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FeXport</servlet-name>
        <url-pattern>/FeXport.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeXportNew</servlet-name>
        <url-pattern>/FeXportNew.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllParams</servlet-name>
        <url-pattern>/GetAllParams</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
